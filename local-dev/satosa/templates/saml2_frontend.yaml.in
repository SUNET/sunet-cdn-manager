---
name: Saml2IDP
config:
  entityid_endpoint: true
  endpoints:
    single_sign_on_service:
      urn:oasis:names:tc:SAML:2.0:bindings:HTTP-POST: sso/post
      urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect: sso/redirect
  idp_config:
    key_file: frontend.key
    cert_file: frontend.crt
    metadata:
      local:
      - metadata/keycloak_sp_metadata.xml
    entityid: %%BASE_URL%%/idp
    service:
      idp:
        endpoints:
          single_sign_on_service: []
        name: SWAMID
        name_id_format:
        - urn:oasis:names:tc:SAML:2.0:nameid-format:transient
        policy:
          default:
            attribute_restrictions:
            fail_on_missing_requested: false
            lifetime:
              minutes: 15
            name_form: urn:oasis:names:tc:SAML:2.0:attrname-format:uri
        want_authn_requests_signed: true
        entity_categories:
        - refeds
    xmlsec_binary: "/usr/bin/xmlsec1"
  attribute_profile: saml
module: satosa.frontends.saml2.SAMLFrontend
plugin: FrontendModulePlugin
